#include <stdio.h>
int main() {
    int n, i, sum = 0, fact;
    printf("Enter a number: ");
    scanf("%d", &n);
    for (i = 1; i <= n; i++) {
        fact = 1;
        for (int j = 1; j <= i; j++) {
            fact *= j;
        }
        sum += fact;
    }
    if (sum == n) {
        printf("%d is a strong number.\n", n);
    } else {
        printf("%d is not a strong number.\n", n);
    }
    return 0;
}
